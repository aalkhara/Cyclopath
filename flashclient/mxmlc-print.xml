        /ccp/opt/flex_util/purePDF/purePDF.swc
      </path-element>
      <path-element>
        /ccp/opt/flex_util/purePDF/purePDFont.swc
      </path-element>
    </library-path>
   <!--
    <include-libraries
      append="true">
      <!- - Keep the original location in the libpath for
           backwards-compatibility. - ->
      <!- -
      <library>
        /ccp/opt/flex_util/as3corelib/as3corelib.swc
